{
  "name": "tsed-example-prisma",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@prisma/generator-helper": "5.3.1",
    "@prisma/sdk": "4.0.0",
    "@prisma/client": "5.3.1",
    "@tsed/ajv": "7.36.5",
    "@tsed/prisma": "7.36.5",
    "@tsed/common": "^7.36.5",
    "@tsed/core": "^7.36.5",
    "@tsed/di": "^7.36.5",
    "@tsed/exceptions": "^7.36.5",
    "@tsed/json-mapper": "^7.36.5",
    "@tsed/platform-express": "^7.36.5",
    "@tsed/schema": "^7.36.5",
    "@tsed/swagger": "^7.36.5",
    "@tsed/typeorm": "^7.36.5",
    "ajv": "7.2.4",
    "barrelsby": "2.8.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.1",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "prisma": "^5.2.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.6.4",
    "@tsed/cli-plugin-jest": "4.6.4",
    "@tsed/cli-plugin-typeorm": "4.6.4",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "29.5.5",
    "@types/method-override": "^0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "postgres": "3.3.5",
    "jest": "29.7.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
