{
  "name": "tsed-example-prisma",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@tsedio/prisma": "1.1.1",
    "@prisma/generator-helper": "2.22.1",
    "@prisma/sdk": "2.22.1",
    "@prisma/client": "2.22.1",
    "@tsed/ajv": "^6.48.0",
    "@tsed/common": "^6.48.0",
    "@tsed/core": "^6.48.0",
    "@tsed/di": "^6.48.0",
    "@tsed/exceptions": "^6.48.0",
    "@tsed/json-mapper": "^6.48.0",
    "@tsed/platform-express": "^6.48.0",
    "@tsed/schema": "^6.48.0",
    "@tsed/swagger": "^6.48.0",
    "@tsed/typeorm": "^6.48.0",
    "ajv": "7.2.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.1",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "pg": "^8.6.0",
    "prisma": "2.22.1",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.0.3",
    "@tsed/cli-plugin-jest": "3.0.3",
    "@tsed/cli-plugin-typeorm": "3.0.3",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/method-override": "^0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
