{
  "name": "tsed-example-prisma",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run barrels && swc src --out-dir dist -s  --strip-leading-paths",
    "barrels": "barrels",
    "start": "npm run barrels && nodemon src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node --import @swc-node/register/esm-register src/index.js",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.0",
    "@swc/helpers": "^0.5.15",
    "@tsed/ajv": "8.3.1",
    "@tsed/barrels": "^6.1.1",
    "@tsed/core": "8.3.1",
    "@tsed/di": "8.3.1",
    "@tsed/engines": "^8.3.1",
    "@tsed/exceptions": "8.3.1",
    "@tsed/json-mapper": "8.3.1",
    "@tsed/logger": "^7.0.1",
    "@tsed/openspec": "8.3.1",
    "@tsed/platform-cache": "8.3.1",
    "@tsed/platform-exceptions": "8.3.1",
    "@tsed/platform-express": "8.3.1",
    "@tsed/platform-http": "8.3.1",
    "@tsed/platform-log-request": "8.3.1",
    "@tsed/platform-middlewares": "8.3.1",
    "@tsed/platform-params": "8.3.1",
    "@tsed/platform-response-filter": "8.3.1",
    "@tsed/platform-views": "8.3.1",
    "@tsed/prisma": "8.3.1",
    "@tsed/schema": "8.3.1",
    "@tsed/swagger": "8.3.1",
    "ajv": "^8.17.1",
    "body-parser": "^1.20.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.2",
    "method-override": "^3.0.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "6.1.1",
    "@tsed/cli-plugin-prisma": "6.1.1",
    "@tsed/cli-plugin-vitest": "6.1.1",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/method-override": "^3.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "prisma": "^6.0.1",
    "supertest": "^7.0.0",
    "tslib": "^2.8.1",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.8"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "npm",
    "runtime": "node"
  },
  "type": "module"
}
